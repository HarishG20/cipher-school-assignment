#include <iostream>
#include <vector>
using namespace std;
int playGame(vector<int>& a) {
    int n = a.size();
    bool isTurtleTurn = true;
    for (int i = 1; i < n; ++i) {
        if (isTurtleTurn) {
            a[0] = max(a[0], a[1]);
        } else {
            a[0] = min(a[0], a[1]);
        }
        isTurtleTurn = !isTurtleTurn; 
        a.erase(a.begin() + 1);  
    }
    return a[0];
}
int main() {
    int t;
    cin >> t;
    while (t--) {
        int n;
        cin >> n;
        vector<int> a(n);
        for (int i = 0; i < n; ++i) {
            cin >> a[i];
        }
        cout << playGame(a) << endl;
    }
}
