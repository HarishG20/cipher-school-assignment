//question-1
#include<iostream>
using namespace std;
int findMax(const int arr[],int size){
    int max=arr[0];
    for(int i=1;i<size;++i){
        if(arr[i]>max){
            max=arr[i];
        }
    }
    return max;
}
int main(){
    int arr1[]={1,5,3,9,2};
    cout<<"Maximum in arr1: "<<findMax(arr1,5)<<endl;
}
