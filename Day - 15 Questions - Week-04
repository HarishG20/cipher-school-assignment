//question-1
#include <iostream>
using namespace std;
class Student {
public:
    string name;
    int age;
    Student(string n, int a) : name(n), age(a) {}
    void displayDetails() {
        cout << "Name: " << this->name << ", Age: " << this->age << endl;
    }
};
int main() {
    Student student1("Alice", 20);
    student1.displayDetails();
    return 0;
}
